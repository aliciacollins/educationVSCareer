<div class="container-fluid" id="logInPage">
    <div class="row">
        <img src="/images/Career Center Logo.png" id="logInLogo" alt="logo">
        <div class="col-md-6 col-md-offset-3" id="logInForm">
            <h2>Login Form</h2>
            <form class="login">
                <div class="form-group">
                    <label for="exampleInputEmail1">Email address</label>
                    <input type="email" class="form-control" id="email-input" placeholder="Email">
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword1">Password</label>
                    <input type="password" class="form-control" id="password-input" placeholder="Password">
                </div>
                <button type="submit" class="btn btn-default" id="logInButton">Login</button>
            </form>
            <br />
            <p>Or sign up
                <a href="/">here</a>
            </p>
        </div>
    </div>
</div>
<script type="text/javascript" src="js/login.js"></script>